    "vscode-languageserver": "^4.0.0"
  },
  "devDependencies": {
    "@angular/compiler": "^5.2.9",
    "@angular/compiler-cli": "^5.2.9",
    "@angular/core": "^5.2.9",
    "@types/jasmine": "^2.8.6",
    "@types/node": "^6.0.46",
    "jasmine": "^3.1.0",
    "rxjs": "^5.5.8",
    "zone.js": "^0.8.20"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/angular/vscode-ng-language-service"
  }
