{
  "name": "ng-template",
  "displayName": "Angular Language Service",
  "description": "Editor services for Angular templates",
  "version": "0.802.2",
  "publisher": "Angular",
  "icon": "angular.png",
  "license": "MIT",
  "keywords": [
    "Angular",
    "multi-root ready"
  ],
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:html",
    "onLanguage:typescript"
  ],
  "main": "./client/out/extension",
  "scripts": {
    "vscode:prepublish": "cd client && yarn update-vscode && cd .. && yarn compile",
    "compile": "tsc -b",
    "watch": "tsc -b -w",
    "postinstall": "cd client && yarn && cd ../server && yarn && cd ..",
    "package": "rm -rf dist && node scripts/package.js",
    "test": "tsc -b server/tests && jasmine server/out/tests/*_spec.js",
    "test:integration": "tsc -b integration && jasmine integration/out/*_spec.js"
  },
  "devDependencies": {
    "@types/jasmine": "^3.4.0",
    "@types/node": "^10.9.4",
    "jasmine": "^3.4.0",
    "typescript": "~3.5.3",
    "vsce": "^1.66.0",
    "vscode-jsonrpc": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/angular/vscode-ng-language-service"
  }
}
