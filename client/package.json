{
  "name": "ng-template",
  "displayName": "Angular Language Service",
  "description": "Editor services for Angular templates",
  "version": "0.800.0-rc.0",
  "publisher": "Angular",
  "icon": "angular.png",
  "keywords": [
    "Angular",
    "multi-root ready"
  ],
  "engines": {
    "vscode": "^1.20.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:ng-template",
    "onLanguage:html",
    "onLanguage:typescript"
  ],
  "main": "./out/extension",
  "contributes": {
    "languages": [
      {
        "id": "ng-template",
        "aliases": [
          "Angular Template",
          "ng",
          "ng-html"
        ],
        "extensions": [
          ".ng",
          ".ngml",
          ".ng.html"
        ],
        "configuration": "./language-configuration.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "update-vscode": "node ./node_modules/vscode/bin/install",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/node": "^10.9.4",
    "typescript": "~3.4.2",
    "vscode": "^1.1.34",
    "vsce": "^1.61.0"
  },
  "dependencies": {
    "vscode-languageclient": "~5.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/angular/vscode-ng-language-service"
  }
}
